{
  "id": "my-api",
  "name": "my-api",
  "gravitee": "2.0.0",
  "proxy": {
    "context_path": "/test",
    "endpoints": [
      {
        "name": "default",
        "target": "http://localhost:8080/team",
        "http": {
          "connectTimeout": 3000,
          "readTimeout": 60000
        }
      }
    ]
  },
  "flows": [
    {
      "name": "ALL",
      "methods": [
        "GET",
        "POST",
        "PUT"
      ],
      "path-operator": {
        "path": "/",
        "operator": "STARTS_WITH"
      },
      "pre": [
        {
          "policy": "key-less",
          "name": "Rate Limit",
          "description": "Step description",
          "configuration": {}
        }
      ],
      "post": [
        {
          "policy": "key-less",
          "name": "url-rewriting",
          "description": "Description of the URL Rewriting Gravitee Policy",
          "configuration": {}
        }
      ]
    }
  ]
}
